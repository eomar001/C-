#include<iostream>
#include<vector>
#include<string>
using std::endl;

int main(){
  std::string input="hello whales";
  
  std::vector<char> vowels={'a','i','o','u','e'};
  
  std:: vector <char> result;
  for(int i =0; i<input.size(); i++){
    for(int j=0;  j<vowels.size(); j++){
      if(input[i] ==vowels[j]){
        result.push_back(input[i]);
      }
      
    }
  }
for (int k = 0; k < result.size(); k++) {

  std::cout << result[k];

}
  std::cout<<endl;
  }
  
                               
                              
                               
